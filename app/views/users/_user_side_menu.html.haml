.user_mypage__container__side
  .user_mypage__container__side__menu
    %ul.user_mypage__container__side__menu__list
      %li.user_mypage__container__side__menu__list__product
        = link_to mypage_path, class: 'user_mypage__container__side__menu__list__product__text-active' do
          マイページ
          %i.fas.fa-chevron-right
      %li.user_mypage__container__side__menu__list__product
        = link_to "#", class: 'user_mypage__container__side__menu__list__product__text' do
          お知らせ
          %i.fas.fa-chevron-right
      %li.user_mypage__container__side__menu__list__product
        = link_to "#", class: 'user_mypage__container__side__menu__list__product__text' do
          やることリスト
          %i.fas.fa-chevron-right
      %li.user_mypage__container__side__menu__list__product
        = link_to "#", class: 'user_mypage__container__side__menu__list__product__text' do
          いいね！一覧
          %i.fas.fa-chevron-right
      %li.user_mypage__container__side__menu__list__product
        = link_to new_product_path, class: 'user_mypage__container__side__menu__list__product__text', data: {"turbolinks" => false} do
          出品する
          %i.fas.fa-chevron-right
      %li.user_mypage__container__side__menu__list__product
        = link_to "#", class: 'user_mypage__container__side__menu__list__product__text' do
          出品した商品 - 出品中
          %i.fas.fa-chevron-right
      %li.user_mypage__container__side__menu__list__product
        = link_to "#", class: 'user_mypage__container__side__menu__list__product__text' do
          出品した商品 - 取引中
          %i.fas.fa-chevron-right
      %li.user_mypage__container__side__menu__list__product
        = link_to "#", class: 'user_mypage__container__side__menu__list__product__text' do
          出品した商品 - 売却済み
          %i.fas.fa-chevron-right
      %li.user_mypage__container__side__menu__list__product
        = link_to "#", class: 'user_mypage__container__side__menu__list__product__text' do
          購入した商品 - 取引中
          %i.fas.fa-chevron-right
      %li.user_mypage__container__side__menu__list__product
        = link_to "#", class: 'user_mypage__container__side__menu__list__product__text' do
          購入した商品 - 過去の取引
          %i.fas.fa-chevron-right
      %li.user_mypage__container__side__menu__list__product
        = link_to "#", class: 'user_mypage__container__side__menu__list__product__text' do
          ニュース一覧
          %i.fas.fa-chevron-right
      %li.user_mypage__container__side__menu__list__product
        = link_to "#", class: 'user_mypage__container__side__menu__list__product__text' do
          評価一覧
          %i.fas.fa-chevron-right
      %li.user_mypage__container__side__menu__list__product
        = link_to "#", class: 'user_mypage__container__side__menu__list__product__text' do
          ガイド
          %i.fas.fa-chevron-right
      %li.user_mypage__container__side__menu__list__product
        = link_to "#", class: 'user_mypage__container__side__menu__list__product__text' do
          お問い合わせ
          %i.fas.fa-chevron-right

    %h3.user_mypage__container__side__menu__title メルペイ
    %ul.user_mypage__container__side__menu__list
      %li.user_mypage__container__side__menu__list__product
        = link_to "#", class: 'user_mypage__container__side__menu__list__product__text' do
          売り上げ・振込申請
          %i.fas.fa-chevron-right
      %li.user_mypage__container__side__menu__list__product
        = link_to "#", class: 'user_mypage__container__side__menu__list__product__text' do
          ポイント
          %i.fas.fa-chevron-right

    %h3.user_mypage__container__side__menu__title 設定
    %ul.user_mypage__container__side__menu__list
      %li.user_mypage__container__side__menu__list__product
        = link_to edit_user_user_profile_path(current_user, 1), class: 'user_mypage__container__side__menu__list__product__text' do
          プロフィール
          %i.fas.fa-chevron-right
      %li.user_mypage__container__side__menu__list__product
        = link_to "#", class: 'user_mypage__container__side__menu__list__product__text' do
          発送元・お届け先住所変更
          %i.fas.fa-chevron-right
      %li.user_mypage__container__side__menu__list__product
        = link_to user_cards_path(current_user), class: 'user_mypage__container__side__menu__list__product__text' do
          支払い方法
          %i.fas.fa-chevron-right
      %li.user_mypage__container__side__menu__list__product
        = link_to "#", class: 'user_mypage__container__side__menu__list__product__text' do
          メール/パスワード
          %i.fas.fa-chevron-right
      %li.user_mypage__container__side__menu__list__product
        = link_to edit_user_user_confirmation_path(current_user, 1), class: 'user_mypage__container__side__menu__list__product__text' do
          本人情報
          %i.fas.fa-chevron-right
      %li.user_mypage__container__side__menu__list__product
        = link_to "#", class: 'user_mypage__container__side__menu__list__product__text' do
          電話番号の確認
          %i.fas.fa-chevron-right
      %li.user_mypage__container__side__menu__list__product
        = link_to new_user_logout_path(current_user), class: 'user_mypage__container__side__menu__list__product__text' do
          ログアウト
          %i.fas.fa-chevron-right